{
  "name": "movie-os-collection",
  "version": "1.0.0",
  "description": "A comprehensive collection of computer interfaces from movies (1968-2024) recreated for the web. Features GSAP animations, Swiss Style design, full i18n support, and responsive components. Built with Nuxt 4 and Vue 3.",
  "author": "Manuel Hintermayr - https://github.com/manuelhintermayr/movie-os-collection",
  "homepage": "https://projects.manuelhintermayr.com/movie-os-collection",
  "repository": {
    "type": "git",
    "url": "https://github.com/manuelhintermayr/movie-os-collection.git"
  },
  "keywords": [
    "movie",
    "computer",
    "interfaces",
    "web",
    "collection",
    "nuxt4",
    "vue3",
    "gsap",
    "swiss-style",
    "ui-recreation",
    "internationalization",
    "responsive-design",
    "dark-mode",
    "search-functionality",
    "movie-ui",
    "film-interfaces",
    "retro-computing",
    "sci-fi-ui"
  ],
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "check:unused-vars": "npx tsc --noEmit --noUnusedLocals --noUnusedParameters",
    "check:unused-vars-enhanced": "node scripts/check-unused-vars-enhanced.js",
    "analyze:i18n": "node scripts/analyze-i18n-strings.js",
    "cleanup:i18n": "node scripts/cleanup-i18n-strings.js",
    "lint:check": "npm run check:unused-vars && npm run analyze:i18n"
  },
  "dependencies": {
    "@internationalized/date": "^3.8.2",
    "@nuxt/ui": "^3.3.2",
    "@nuxtjs/i18n": "^10.0.6",
    "gsap": "^3.13.0",
    "mouse-follower": "^1.1.2",
    "nuxt": "^4.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2"
  }
}
